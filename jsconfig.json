{
    "compilerOptions": {
      "baseUrl": "src",
      "paths": {
        "api/*": ["api/*"],
        "assets/*": ["assets/*"],
        "components/*": ["components/*"],
        "hooks/*": ["hooks/*"],
        "pages/*": ["pages/*"],
        "state":[ "state/*"],
        "utility/*": ["utility/*"]
      },
      "target": "es6",
      "module": "commonjs",
      "jsx": "react",
      "esModuleInterop": true
    },
    "include": ["src/**/*.js", "src/**/*.jsx", "src/**/*.ts", "src/**/*.tsx"],
    "exclude": ["node_modules", "build", "dist"]
  }